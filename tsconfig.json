{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@env": [
        "./src/core/env.js"
      ]
    },
    "esModuleInterop": true,
    "checkJs": true
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "docs",
    "cli",
    "android",
    "ios"
  ],
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
